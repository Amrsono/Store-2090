{
    "rewrites": [
        {
            "source": "/graphql",
            "destination": "/api/index.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "includeFiles": "backend/**",
            "maxDuration": 10,
            "memory": 1024
        }
    }
}